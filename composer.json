{
  "name": "tourze/env-manage-bundle",
  "description": "环境变量管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/dotenv": "^6.4",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/arrayable": "0.0.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-ip-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*",
    "psr/log": "^3|^2|^1",
    "twig/twig": "^3.13|^4.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EnvManageBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EnvManageBundle\\Tests\\": "tests"
    }
  }
}